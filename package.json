{
	"name": "national-group-construction",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"server": "node server/index.js",
		"dev:all": "concurrently \"npm run server\" \"npm run dev\"",
		"start": "npm run server",
		"build:server": "echo 'Server build complete'",
		"postbuild": "cp -r server build/ || copy server build\\ /E"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@types/bcryptjs": "^2.4.6",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.3",
		"@types/jsonwebtoken": "^9.0.10",
		"concurrently": "^9.2.1",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"bcryptjs": "^3.0.2",
		"cors": "^2.8.5",
		"date-fns": "^4.1.0",
		"dotenv": "^17.2.2",
		"express": "^5.1.0",
		"jsonwebtoken": "^9.0.2",
		"lucide-svelte": "^0.542.0",
		"serverless-http": "^4.0.0",
		"sqlite3": "^5.1.7"
	}
}
